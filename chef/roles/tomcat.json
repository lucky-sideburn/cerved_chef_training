{
  "name": "tomcat",
  "description": "Install and configure Jenkins",
  "json_class": "Chef::Role",
  "default_attributes": {

  },
  "override_attributes": {
    "my_docker": {

      "image": "tomcat",

      "containers": [
        {
          "name": "tomcat1",
          "ports": "8080:8080",
          "files": "/vagrant/webapps:/usr/local/tomcat/webapps"
        },
        {
          "name": "tomcat2",
          "ports": "8082:8080",
          "files": "/vagrant/webapps:/usr/local/tomcat/webapps"
        }
      ]
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[my_docker]"
  ],
  "env_run_lists": {

  }
}
